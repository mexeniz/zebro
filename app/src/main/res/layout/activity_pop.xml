<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.zebro.isel.zebro.PopActivity"
    android:alpha="1">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="&quot;Let Zebro be your bro!&quot;"
        android:id="@+id/quote"
        android:singleLine="false"
        android:shadowColor="#6b5a5a"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:textIsSelectable="false"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:textStyle="bold" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton"
        android:src="@drawable/abc_ic_clear_mtrl_alpha"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:onClick="onCloseClicked"/>

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="ISEL Lab inc."
        android:id="@+id/company"
        android:textSize="15dp"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:layout_alignParentEnd="false"
        android:layout_alignParentStart="false"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="50dp"
        android:textIsSelectable="false" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/zebro_eye_credit" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        android:collapseColumns="2"
        android:layout_below="@+id/quote"
        android:layout_above="@+id/company">

        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:gravity="right">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Created by"
                android:id="@+id/createdTtile"
                android:layout_weight="1"
                android:singleLine="false"
                android:shadowColor="#6b5a5a"
                android:textSize="12dp"
                android:textColor="#ffffff"
                android:layout_below="@+id/quote"
                android:layout_alignParentEnd="true"
                android:textStyle="italic"
                android:gravity="right" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pawissakan Chirupphapa"
                android:id="@+id/creator1"
                android:layout_weight="1"
                android:singleLine="false"
                android:shadowColor="#6b5a5a"
                android:textSize="15dp"
                android:textColor="#ffffff"
                android:layout_below="@+id/quote"
                android:layout_alignParentEnd="true"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:gravity="left" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"></FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sanchai Jaktheerangkoon"
                android:layout_weight="1"
                android:id="@+id/creator2"
                android:singleLine="false"
                android:shadowColor="#6b5a5a"
                android:textSize="15dp"
                android:textColor="#ffffff"
                android:layout_below="@+id/quote"
                android:layout_marginTop="5dp"
                android:layout_alignParentEnd="true"
                android:layout_marginLeft="10dp"
                android:textStyle="bold" />

        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:gravity="right"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Icons from"
                android:id="@+id/iconFrom"
                android:layout_weight="1"
                android:singleLine="false"
                android:shadowColor="#6b5a5a"
                android:textSize="12dp"
                android:textColor="#ffffff"
                android:layout_below="@+id/quote"
                android:layout_alignParentEnd="true"
                android:textStyle="italic"
                android:gravity="right" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Material Design Icon"
                android:id="@+id/icon"
                android:singleLine="false"
                android:layout_weight="1"
                android:shadowColor="#6b5a5a"
                android:textSize="15dp"
                android:textColor="#ffffff"
                android:layout_below="@+id/quote"
                android:layout_alignParentEnd="true"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:gravity="left" />
        </TableRow>
        <TableRow
            android:layout_height="wrap_content"
            android:layout_width="fill_parent"
            android:gravity="right"
            android:layout_marginTop= "10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sounds from"
                android:id="@+id/soundFrom"
                android:layout_weight="1"
                android:singleLine="false"
                android:shadowColor="#6b5a5a"
                android:textSize="12dp"
                android:textColor="#ffffff"
                android:layout_below="@+id/quote"
                android:layout_alignParentEnd="true"
                android:textStyle="italic"
                android:gravity="right" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Android Beat"
                android:id="@+id/ab"
                android:singleLine="false"
                android:layout_weight="1"
                android:shadowColor="#6b5a5a"
                android:textSize="15dp"
                android:textColor="#ffffff"
                android:layout_below="@+id/quote"
                android:layout_alignParentEnd="true"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:gravity="left" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"></FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="freeSFX"
                android:layout_weight="1"
                android:id="@+id/freeSFX"
                android:singleLine="false"
                android:shadowColor="#6b5a5a"
                android:textSize="15dp"
                android:textColor="#ffffff"
                android:layout_below="@+id/quote"
                android:layout_alignParentEnd="true"
                android:layout_marginLeft="10dp"
                android:textStyle="bold" />

        </TableRow>
    </TableLayout>

